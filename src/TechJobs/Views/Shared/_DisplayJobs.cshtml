@foreach (var job in Model.Jobs)
{
    <table class="job-listing">
        <tr>
            <td>Name</td>
            <td>@job.Name</td>
        </tr>
        <tr>
            <td>Employer</td>
            <td><a asp-controller="List" asp-action="Jobs" asp-route-column="Employer" asp-route-value="@job.Employer.ToString()">@job.Employer.ToString()</a></td>
        </tr>
        <tr>
            <td>Location</td>
            <td><a asp-controller="List" asp-action="Jobs" asp-route-column="Location" asp-route-value="@job.Location.ToString()">@job.Location.ToString()</a></td>
        </tr>
        <tr>
            <td>Skill</td>
            <td><a asp-controller="List" asp-action="Jobs" asp-route-column="CoreCompetency" asp-route-value="@job.CoreCompetency.ToString()">@job.CoreCompetency.ToString()</a></td>
        </tr>
        <tr>
            <td>Position Type</td>
            <td><a asp-controller="List" asp-action="Jobs" asp-route-column="PositionType" asp-route-value="@job.PositionType.ToString()">@job.PositionType.ToString()</a></td>
        </tr>
    </table>
}